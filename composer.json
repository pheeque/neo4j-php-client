{
    "name": "graphaware/neo4j-php-client",
    "type": "library",
    "description": "Neo4j-PHP-Client is the most advanced PHP Client for Neo4j",
    "keywords": [
        "graph",
        "database",
        "neo4j",
        "cluster",
        "client",
        "bolt",
        "http",
        "high-availability"
    ],
    "homepage": "http://graphaware.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Christophe Willemsen",
            "email": "christophe@graphaware.com"
        }
    ],
    "require": {
      "php": "^5.6 || ^7.0",
      "ext-bcmath": "*",
      "ext-mbstring": "*",
      "graphaware/neo4j-common": "*",
      "graphaware/neo4j-bolt": "*",
      "symfony/event-dispatcher": "*",
      "myclabs/php-enum": "*",
      "php-http/httplug": "*",
      "php-http/message-factory": "*",
      "php-http/client-common": "*",
      "php-http/discovery": "*",

      "php-http/message": "*",
      "php-http/guzzle6-adapter": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "*",
        "phpunit/phpunit": "*",
        "symfony/stopwatch": "*"
    },
    "autoload": {
        "psr-4": {
            "GraphAware\\Neo4j\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GraphAware\\Neo4j\\Client\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
